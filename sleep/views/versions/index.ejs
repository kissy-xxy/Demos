<h2>版本</h2>
<hr>
<a href="/versions/new" class="btn btn-success pull-left">新建</a>
<table class="table table-hover version_table  table-striped table-condensed">
	<thead>
		<tr>
			<th></th>
			<th>ID</th>
			<th>应用</th>
			<th>版本号</th>
			<th>操作系统</th>
			<th>强制更新</th>
			<th>创建时间</th>
			<th></th>
		</tr>
	</thead>
	<%_.each(versions,function(version){%>
	<tr>
		<td style="width:200px">
			<a href="<%= '/versions/show/'+version.id%>" class="btn btn-default inline">查看</a>
			<a href="<%= '/versions/edit/'+version.id%>" class="btn btn-primary">编辑</a>
			<a href="<%= '/versions/destroy/'+version.id%>" class="btn btn-danger">删除</a>
		</td>
		<td style="width:70px;"><%=version.id%></td>
		<td style="width:70px;"><%=version.app&&version.app.name||'未指定'%></td>
		<td style="width:80px;"><%=version.number%></td>
		<td style="width:80px;"><%=version.os_type%></td>
		<td style="width:80px;"><%=version.mode?'是':'否'%></td>
		<td><%=version.createdAt.Format("yyyy-MM-dd HH:mm:ss")%></td>
		<td><a href="<%=version.download_url%>">下载</a> </td>
		
	</td>
</tr>
<%})%>
</table>